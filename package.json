{
  "name": "serverless-typescript-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "tslint:prettier-check": "tslint-config-prettier-check ./tslint.json",
    "tslint": "tslint --fix --force --format stylish --project ./tsconfig.json ./*.{ts,tsx}",
    "tslint:ci": "tslint --fix --force --format stylish --project ./tsconfig.json --format junit --out reports/tslint/test-results.xml ./*.{ts,tsx}",
    "prettier": "prettier --write ./*.{ts,tsx}",
    "test": "mocha --require ts-node/register test/**/*.ts",
    "test:ci": "mocha --require ts-node/register --reporter mocha-circleci-reporter --reporter-options mochaFile=reports/mocha/test-results.xml test/**/*.ts"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.22",
    "@types/aws-sdk": "^2.7.0",
    "@types/boom": "^7.2.1",
    "@types/express": "^4.16.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.11.1",
    "@types/webpack": "^4.4.25",
    "aws-lambda": "^0.1.2",
    "aws-sdk-mock": "^4.4.0",
    "aws-serverless-express": "^3.3.5",
    "mocha": "^6.1.3",
    "mocha-circleci-reporter": "^0.0.3",
    "prettier": "^1.16.4",
    "serverless": "^1.38.0",
    "serverless-offline": "^4.9.0",
    "serverless-webpack": "^5.2.0",
    "ts-loader": "^5.3.3",
    "tslint": "^5.14.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.3.3333",
    "webpack": "^4.29.6",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "aws-sdk": "^2.419.0",
    "body-parser": "^1.18.3",
    "boom": "^7.3.0",
    "express": "^4.16.4",
    "express-async-handler": "^1.1.4",
    "serverless-http": "^1.9.1"
  }
}
