{
  "name": "serverless-typescript-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "eslint": "eslint --fix ./*.ts ./utils/**/*.ts ./test/**/*.ts",
    "eslint:ci": "eslint --fix --format junit --output-file reports/eslint/test-results.xml ./*.ts ./utils/**/*.ts ./test/**/*.ts",
    "prettier": "prettier --write ./*.{ts,tsx}",
    "test": "mocha --require ts-node/register test/**/*.ts",
    "test:ci": "mocha --require ts-node/register --reporter mocha-circleci-reporter --reporter-options mochaFile=reports/mocha/test-results.xml test/**/*.ts"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.61",
    "@types/aws-sdk": "^2.7.0",
    "@types/boom": "^7.3.0",
    "@types/express": "^4.17.7",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.6.4",
    "@types/webpack": "^4.41.21",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "aws-lambda": "^1.0.6",
    "aws-sdk-mock": "^5.1.0",
    "aws-serverless-express": "^3.3.8",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "mocha": "^7.2.0",
    "mocha-circleci-reporter": "^0.0.3",
    "prettier": "^2.1.1",
    "serverless": "^1.80.0",
    "serverless-offline": "^5.12.1",
    "serverless-webpack": "^5.3.4",
    "ts-loader": "^8.0.3",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-node-externals": "^2.5.2"
  },
  "dependencies": {
    "aws-sdk": "^2.742.0",
    "body-parser": "^1.18.3",
    "boom": "^7.3.0",
    "express": "^4.16.4",
    "express-async-handler": "^1.1.4",
    "serverless-http": "^2.5.0"
  }
}
